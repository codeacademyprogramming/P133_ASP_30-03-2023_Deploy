@model IEnumerable<Setting>

<div class="row">
    <div class="col-lg-2">
        <h1 class="h3 mb-4 text-gray-800">Categories Page</h1>
    </div>
    <div class="col-lg-2">
        <a class="btn btn-primary" asp-action="create" asp-controller="Category" asp-area="manage"> Create</a>
    </div>
</div>
<div class=" row">
    <div class="col-lg-12">
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>№</th>
                    <th>Key</th>
                    <th>Value</th>
                    <th>Settings</th>
                </tr>
            </thead>
            <tbody>
                @await Html.PartialAsync("_SettingIndexPartial",Model)
            </tbody>
        </table>
    </div>
</div>


@section Scripts{
    @await Html.PartialAsync("_Validation")
}